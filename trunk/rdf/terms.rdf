  <!-- Breeding event -->

  <skos:Concept rdf:about="&germplasm;breedingID">
    <skos:prefLabel xml:lang="en">breeders ID</skos:prefLabel>
    <skos:definition xml:lang="en">Persistent identifier for the source germplasm developed by a plant breeder.</skos:definition>
    <skos:example xml:lang="en">Persistent identifier (PID) such as a URI, DOI, LSID or similar.</skos:example>
    <skos:note xml:lang="en">Local identifiers used by the breeder is best reported using the breedingIdentifier term.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/breedingID"/>
