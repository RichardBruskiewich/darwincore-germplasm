  <skos:Concept rdf:about="&germplasm;breedingYear">
    <skos:prefLabel xml:lang="en">breeding year</skos:prefLabel>
    <skos:definition xml:lang="en">The year a germplasm accession was developed by a plant breeder. Could be the year the cultivar was released.</skos:definition>
    <skos:example xml:lang="en">2010</skos:example>
    <skos:note xml:lang="en">The four-digit year in which the breeding event was conducted.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/breedingYear"/>
