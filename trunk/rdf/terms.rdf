    <dcterms:replaces rdf:resource="&germplasm01;PurdyPedigree"/>
    <rdfs:domain rdf:resource="&germplasm;BreedingEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;breedingRemarks">
    <skos:prefLabel xml:lang="en">breeding remarks</skos:prefLabel>
    <skos:definition xml:lang="en">Remarks to the breeding event.</skos:definition>
    <skos:example xml:lang="en">Supplementary comments and remarks related to the breeding event</skos:example>
    <skos:note xml:lang="en">The breeding event is related to the development of germplasm by a breeder or through cultivation by a farmer depending on the biological status of the germplasm accession.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/breedingRemarks"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;BreedingEvent"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;BreedingEventRemarks"/>
    <rdfs:domain rdf:resource="&germplasm;BreedingEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </skos:Concept>



  <!-- Acquisition -->

  <skos:Concept rdf:about="&germplasm;acquistionID">
    <skos:prefLabel xml:lang="en">acquisition ID</skos:prefLabel>
    <skos:definition xml:lang="en">An identifier for the event when germplasm material was acquired and included in the genebank collection.</skos:definition>
    <skos:example xml:lang="en">URI; DOI; LSID or similar</skos:example>
    <skos:note xml:lang="en">The identifier used by the donor to identify the germplasm specimen is reported using the donorsID term.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/acquistionID"/>
    <dcterms:created>2012-05-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;AcquisitionEvent"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <skos:narrowMatch rdf:resource="dwc:eventID"/>
    <dcterms:source rdf:resource="http://apps3.fao.org/wiews/mcpd/MCPD_Dec2001_EN.pdf"/>
    <rdfs:domain rdf:resource="&germplasm;AcquisitionEvent"/>
    <rdfs:range rdf:resource="dcterms:identifier"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;donorsID">
    <skos:prefLabel xml:lang="en">donors accession ID</skos:prefLabel>
    <skos:definition xml:lang="en">An identifier for the source germplasm material that was included in your genebank collection.</skos:definition>
    <skos:example xml:lang="en">http://data.gbif.org/occurrences/164390781/; URI; DOI; LSID or similar</skos:example>
    <skos:note xml:lang="en">Local identifiers used by the donor is best reported using the donorsIdentifier term.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/donorsID"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;AcquisitionEvent"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;SampleAcquisitionID"/>
    <skos:relatedMatch rdf:resource="dwc:otherCatalogNumbers"/>
    <skos:closeMatch rdf:resource="http://kos.gbif.org/wiki/germplasm:donorsID"/>
    <dcterms:source rdf:resource="http://apps3.fao.org/wiews/mcpd/MCPD_Dec2001_EN.pdf"/>
    <rdfs:range rdf:resource="dcterms:identifier"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;donorsIdentifier">
    <skos:prefLabel xml:lang="en">donors accession identifier</skos:prefLabel>
    <skos:definition xml:lang="en">An identifier for the source accession donated to your genebank collection. Could be an unique and stable local identifier specific to the dataset.</skos:definition>
    <skos:example xml:lang="en">NGB1</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to report using the donorsID term whenever possible.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/donorsIdentifier"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;AcquisitionEvent"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;DonorsSampleIdentifier"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&germplasm;mcpdTerm">DONORNUMB</skos:altLabel>
    <skos:altLabel xml:lang="en">Donor accession number</skos:altLabel>
    <skos:relatedMatch rdf:resource="dwc:otherCatalogNumbers"/>
    <dcterms:source rdf:resource="http://apps3.fao.org/wiews/mcpd/MCPD_Dec2001_EN.pdf"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;donorInstituteID">
    <skos:prefLabel xml:lang="en">donor institute ID</skos:prefLabel>
    <skos:definition xml:lang="en">Donor institute code, DONORCODE. FAO WIEWS institute code for the donor institute (MCPD term 23).</skos:definition>
    <skos:example xml:lang="en">DEU146; NLD037; SWE054; urn:lsid:biocol.org:col:35293</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to use institute codes from the FAO WIEWS information system, "http://apps3.fao.org/wiews/institute_query.htm?i_l=EN" or the Biodiversity Collections Index (BCI), "http://www.biodiversitycollectionsindex.org/".</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/donorInstituteID"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;AcquisitionEvent"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;DonorInstituteCode"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&germplasm;mcpdTerm">DONORCODE</skos:altLabel>
    <skos:altLabel xml:lang="en">Donor institute code</skos:altLabel>
    <dcterms:source rdf:resource="http://apps3.fao.org/wiews/mcpd/MCPD_Dec2001_EN.pdf"/>
    <rdfs:range rdf:resource="foaf:Organization"/>
    <rdfs:range rdf:resource="dcterms:identifier"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;donorInstitute">
    <skos:prefLabel xml:lang="en">donor institute</skos:prefLabel>
    <skos:definition xml:lang="en">Donor institute name. If the FAO WIEWS institute code is available, this term should not be reported (MCPD term 23).</skos:definition>
    <skos:example xml:lang="en">Nordic Genetic Resources Center (NordGen)</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to report using the donorInstituteID term whenever possible.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/donorInstitute"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;AcquisitionEvent"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;DonorInstitute"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&germplasm;euriscoTerm">DONORDESCR</skos:altLabel>
    <skos:altLabel xml:lang="en">Donor institute</skos:altLabel>
    <dcterms:source rdf:resource="http://www.ecpgr.cgiar.org/fileadmin/www.ecpgr.cgiar.org/MISC/EURISCO_Descriptors.pdf"/>
    <rdfs:range rdf:resource="foaf:Organization"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;acquisitionDate">
    <skos:prefLabel xml:lang="en">acquisition date</skos:prefLabel>
    <skos:definition xml:lang="en">Acquisition date, ACQDATE. Date when the germplasm sample (accession) entered the genebank collection (MCPD term 12).</skos:definition>
    <skos:example xml:lang="en">2010-05-10</skos:example>
    <skos:note xml:lang="en">Accessions can be acquired by a collecting event or from other genebank collections by an acquisition event. For either event this terms describes the date the accession was included into the receiving genebank collection.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/acquisitionDate"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;AcquisitionEvent"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;SampleAcquisitionDate"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&germplasm;mcpdTerm">ACQDATE</skos:altLabel>
    <skos:altLabel xml:lang="en">Acquisition date</skos:altLabel>
    <skos:narrowMatch rdf:resource="dwc:eventDate"/>
    <dcterms:source rdf:resource="http://apps3.fao.org/wiews/mcpd/MCPD_Dec2001_EN.pdf"/>
    <rdfs:domain rdf:resource="&germplasm;AcquisitionEvent"/>
    <rdfs:range rdf:resource="xsd:date"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;acquisitionSource">
    <skos:prefLabel xml:lang="en">acquisition source</skos:prefLabel>
    <skos:definition xml:lang="en">Collecting/acquisition source, COLLSRC. Coded as integer value with defined enumeration (MCPD term 22).</skos:definition>
    <skos:example xml:lang="en">10, 11, 12, 13, 14, 15, 20, 21, 22, 23, 24, 25, 26, 27, 28, 30, 40, 50, 60, 61, 62, 99</skos:example>
    <skos:note xml:lang="en">Recommended to use the MCPD coding scheme. Wild habitat (10), Farm or cultivated habitat (20), Market or shop (30), Institute, experimental station, research organization, genebank (40), Seed company (50), Weedy, disturbed or ruderal habitat (60).</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/acquisitionSource"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;AcquisitionEvent"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;SampleAcquisitionSource"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&germplasm;mcpdTerm">COLLSRC</skos:altLabel>
    <skos:altLabel xml:lang="en">Collecting, acquisition source</skos:altLabel>
    <skos:relatedMatch rdf:resource="dcterms:accrualMethod"/>
    <skos:relatedMatch rdf:resource="dwc:habitat"/>
    <dcterms:source rdf:resource="http://apps3.fao.org/wiews/mcpd/MCPD_Dec2001_EN.pdf"/>
    <rdfs:domain rdf:resource="&germplasm;AcquisitionEvent"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;acquisitionRemarks">
    <skos:prefLabel xml:lang="en">acquisition remarks</skos:prefLabel>
    <skos:definition xml:lang="en">Remarks to the acquisition event.</skos:definition>
    <skos:example xml:lang="en">General remarks related to the acquisition of the germplasm accession.</skos:example>
    <skos:note xml:lang="en">The acquisition terms relates to the immediate step or the germplasm holder before the accession was included in the genebank collection. Information related to the collecting event and the breeding event are not reported here.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/acquisitionRemarks"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;AcquisitionEvent"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;SampleAcquisitionRemarks"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&germplasm;mcpdTerm">REMARKS</skos:altLabel>
    <skos:altLabel xml:lang="en">Remarks</skos:altLabel>
    <dcterms:source rdf:resource="http://apps3.fao.org/wiews/mcpd/MCPD_Dec2001_EN.pdf"/>
    <rdfs:domain rdf:resource="&germplasm;AcquisitionEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </skos:Concept>



  <!-- Safety duplication -->

  <skos:Concept rdf:about="&germplasm;safetyDuplicationID">
    <skos:prefLabel xml:lang="en">safety duplication ID</skos:prefLabel>
    <skos:definition xml:lang="en">Persistent identifier for the genebank accession in the safety duplication storage. A globally unique identifier (GUID), persistent identifier (PI) such as a URI, DOI, LSID or similar is preferred.</skos:definition>
    <skos:example xml:lang="en">URI, DOI, LSID or similar.</skos:example>
    <skos:note xml:lang="en">Could also be an unique and stable identifier specific to the safety storage collection.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/safetyDuplicationID"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;SafetyDuplication"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;SafetyDuplicationID"/>
    <rdfs:range rdf:resource="dcterms:identifier"/>
    <skos:narrower rdf:resource="&germplasm;germplasmID"/>
    <skos:relatedMatch rdf:resource="dwc:otherCatalogNumbers"/>
    <rdfs:domain rdf:resource="&germplasm;SafetyDuplication"/>
    <rdfs:range rdf:resource="dcterms:identifier"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;safetyDuplicationDate">
    <skos:prefLabel xml:lang="en">safety duplication date</skos:prefLabel>
    <skos:definition xml:lang="en">Date of safety duplication.</skos:definition>
    <skos:example xml:lang="en">2008-02-26</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to use an encoding scheme, such as ISO 8601:2004(E).</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/safetyDuplicationDate"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;SafetyDuplication"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;SafetyDuplicationDate"/>
    <skos:narrowMatch rdf:resource="dwc:eventDate"/>
    <rdfs:domain rdf:resource="&germplasm;SafetyDuplication"/>
    <rdfs:range rdf:resource="xsd:date"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;safetyDuplicationInstituteID">
    <skos:prefLabel xml:lang="en">safety duplication institute ID</skos:prefLabel>
    <skos:definition xml:lang="en">Location of safety duplicates. FAO institute code of the institute where a safety duplicate of the accession is maintained (MCPD term 26).</skos:definition>
    <skos:example xml:lang="en">NOR051; urn:lsid:biocol.org:col:35294</skos:example>
    <skos:note xml:lang="en">Recommended best practice  is to use institute codes from the FAO WIEWS information system, "http://apps3.fao.org/wiews/institute_query.htm?i_l=EN" or the Biodiversity Collections Index (BCI), "http://www.biodiversitycollectionsindex.org/".</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/safetyDuplicationInstituteID"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;SafetyDuplication"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;SafetyDuplicationInstituteCode"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&germplasm;mcpdTerm">DUPLSITE</skos:altLabel>
    <skos:altLabel xml:lang="en">Location of safety duplicates</skos:altLabel>
    <skos:relatedMatch rdf:resource="foaf:Organization"/>
    <dcterms:source rdf:resource="http://apps3.fao.org/wiews/mcpd/MCPD_Dec2001_EN.pdf"/>
    <rdfs:range rdf:resource="dcterms:identifier"/>
    <rdfs:range rdf:resource="foaf:Organization"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;safetyDuplicationInstitute">
    <skos:prefLabel xml:lang="en">safety duplication institute</skos:prefLabel>
    <skos:definition xml:lang="en">Location of safety duplicates (MCPD term 26).</skos:definition>
    <skos:example xml:lang="en">The name of the institute holding the safe duplication germplasm material.</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to use the SafetyDuplicationInstituteID term whenever possible.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/safetyDuplicationInstitute"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;SafetyDuplication"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;SafetyDuplicationInstitute"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&germplasm;euriscoTerm">DUPLDESCR</skos:altLabel>
    <skos:altLabel xml:lang="en">Location of safety duplicates</skos:altLabel>
    <dcterms:source rdf:resource="http://www.ecpgr.cgiar.org/fileadmin/www.ecpgr.cgiar.org/MISC/EURISCO_Descriptors.pdf"/>
    <skos:broader rdf:resource="&germplasm;SafetyDuplicationInstituteID"/>
    <rdfs:range rdf:resource="foaf:Organization"/>
  </skos:Concept>




  <!-- Treaty or regulation -->

  <skos:Concept rdf:about="&germplasm;treatyOrRegulationID">
    <skos:prefLabel xml:lang="en">treaty or regulation ID</skos:prefLabel>
    <skos:definition xml:lang="en">An identifier for the regulation or treaty.</skos:definition>
    <skos:example xml:lang="en">URI, DOI, LSID or similar.</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to report a persistent identifier for the treaty or regulation.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/treatyOrRegulationID"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;TreatyOrRegulation"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;GermplasmRegulationID"/>
    <rdfs:range rdf:resource="dcterms:identifier"/>
    <rdfs:domain rdf:resource="&germplasm;TreatyOrRegulation"/>
    <rdfs:range rdf:resource="dcterms:identifier"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;treatyOrRegulationName">
    <skos:prefLabel xml:lang="en">treaty or regulation</skos:prefLabel>
    <skos:definition xml:lang="en">Regulation or treaty name.</skos:definition>
    <skos:example xml:lang="en">The International Treaty for Plant Genetic Resources for Food and Agriculture (ITPGRFA)</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to use the treatyOrRegulationID term when possible.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/treatyOrRegulationName"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;TreatyOrRegulation"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;TreatyOrRegulation"/>
    <rdfs:domain rdf:resource="&germplasm;TreatyOrRegulation"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;treatyOrRegulationGoverningBody">
    <skos:prefLabel xml:lang="en">governing body</skos:prefLabel>
    <skos:definition xml:lang="en">Governing body for the regulation or treaty.</skos:definition>
    <skos:example xml:lang="en">WIEWS institute code</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to use institute codes from the FAO WIEWS information system, "http://apps3.fao.org/wiews/institute_query.htm?i_l=EN" or the Biodiversity Collections Index (BCI), "http://www.biodiversitycollectionsindex.org/".</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/treatyOrRegulationGoverningBody"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;TreatyOrRegulation"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;TreatyOrRegulationGoverningBody"/>
    <skos:relatedMatch rdf:resource="foaf:Group"/>
    <rdfs:domain rdf:resource="&germplasm;TreatyOrRegulation"/>
  </skos:Concept>



  <!-- Measurement method -->

  <skos:Concept rdf:about="&germplasm;traitID">
    <skos:prefLabel xml:lang="en">trait ID</skos:prefLabel>
    <skos:definition xml:lang="en">Persistent identifier for a trait descriptor used to identify the description of the measurement method.</skos:definition>
    <skos:example xml:lang="en">URI, DOI, LSID or similar</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to use a persistent identifier for trait descriptors, and a controlled and curated ontology such as the Plant Ontology (http://www.plantontology.org).</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/traitID"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;MeasurementMethod"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;GermplasmTraitID"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&germplasm;epgrisTerm">TRAIT_NUMBER</skos:altLabel>
    <skos:altLabel xml:lang="en">Trait descriptor number</skos:altLabel>
    <skos:relatedMatch rdf:resource="dwc:measurementMethod"/>
    <dcterms:source rdf:resource="http://www.epgris3.eu/docs/activities/2-05/Inclusion%20of%20C&amp;E%20data.pdf"/>
    <rdfs:range rdf:resource="dcterms:identifier"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;traitIdentifier">
    <skos:prefLabel xml:lang="en">trait identifier</skos:prefLabel>
    <skos:definition xml:lang="en">English name used to identify the trait measurement method.</skos:definition>
    <skos:example xml:lang="en">stem rust susceptibility; powdery mildew susceptibility; plant height; ear density; etc</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to use the GermplasmTraitID term whenever possible, and to use a controlled vocabulary such as the Plant Ontology (http://www.plantontology.org).</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/traitIdentifier"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;MeasurementMethod"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;GermplasmTraitIdentifier"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&germplasm;epgrisTerm">TRAIT_NAME</skos:altLabel>
    <skos:altLabel xml:lang="en">English name of the trait</skos:altLabel>
    <skos:relatedMatch rdf:resource="dwc:measurementMethod"/>
    <dcterms:source rdf:resource="http://www.epgris3.eu/docs/activities/2-05/Inclusion%20of%20C&amp;E%20data.pdf"/>
    <rdfs:range rdf:resource="dcterms:identifier"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;traitGroup">
    <skos:prefLabel xml:lang="en">trait descriptor group</skos:prefLabel>
    <skos:definition xml:lang="en">Classification of the trait descriptor.</skos:definition>
    <skos:example xml:lang="en">agrobotanical; morphological; chemical; phenological; molecular biology; disease susceptibility/-resistance; tolerance (abiotic); utilization.</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to use a controlled vocabulary such as the Plant Ontology (http://www.plantontology.org).</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/traitGroup"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;MeasurementMethod"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;GermplasmTraitClass"/>
    <skos:relatedMatch rdf:resource="dwc:measurementType"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;traitScale">
    <skos:prefLabel xml:lang="en">trait measurement scale</skos:prefLabel>
    <skos:definition xml:lang="en">Measurement scale used to score the measurement values.</skos:definition>
    <skos:example xml:lang="en">absolute; interval; nominal; ordinal; ratio; relative</skos:example>
    <skos:note xml:lang="en">The theory of scale types was developed by Stanley S. Stevens (1946) in Science 103(2684):677‚Äì680.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/traitScale"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;MeasurementMethod"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;GermplasmTraitScale"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;traitSource">
    <skos:prefLabel xml:lang="en">trait descriptor source</skos:prefLabel>
    <skos:definition xml:lang="en">Reference to a standard Germplasm Descriptor. For example the UPOV descriptor or the Bioversity Crop Descriptor identifier.</skos:definition>
    <skos:example xml:lang="en">UPOV TG/198/1 (april 2003)</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to use a controlled vocabulary of source trait descriptors.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/traitSource"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;MeasurementMethod"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;GermplasmTraitSource"/>
    <skos:relatedMatch rdf:resource="dwc:measurementMethod"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;traitRemarks">
    <skos:prefLabel xml:lang="en">trait remarks</skos:prefLabel>
    <skos:definition xml:lang="en">Remarks related to the trait measurement method.</skos:definition>
    <skos:example xml:lang="en">inflorescences after cutting; measured three days after heading date</skos:example>
    <skos:note xml:lang="en">Any general remark that helps interpret the trait measurement method.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/traitRemarks"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;MeasurementMethod"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;GermplasmTraitRemarks"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&germplasm;epgrisTerm">TRAIT_REMARK</skos:altLabel>
    <skos:altLabel xml:lang="en">Measurement remarks</skos:altLabel>
    <dcterms:source rdf:resource="http://www.epgris3.eu/docs/activities/2-05/Inclusion%20of%20C&amp;E%20data.pdf"/>
    <skos:relatedMatch rdf:resource="dwc:measurementRemarks"/>
  </skos:Concept>


  <!-- Measurement experiment -->

  <skos:Concept rdf:about="&germplasm;traitExperimentID">
    <skos:prefLabel xml:lang="en">trait experiment ID</skos:prefLabel>
    <skos:definition xml:lang="en">Persistent identifier used to identify a trait measurement experiment.</skos:definition>
    <skos:example xml:lang="en">URI, DOI, LSID or similar</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to use a persistent identifier for trait measurement experiments.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/traitExperimentID"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;MeasurementExperiment"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;GermplasmExperimentID"/>
    <rdfs:range rdf:resource="dcterms:identifier"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;traitExperimentIdentifier">
    <skos:prefLabel xml:lang="en">trait experiment identifier</skos:prefLabel>
    <skos:definition xml:lang="en">Unique number in the dataset for the experiment.</skos:definition>
    <skos:example xml:lang="en">Local identifier for the experiment or project activity.</skos:example>
    <skos:note xml:lang="en">This number should be unique and persistent for the data provider. Recommended best practice is to use the GermplasmExperimentID term whenever possible.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/traitExperimentIdentifier"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;MeasurementExperiment"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;GermplasmExperimentIdentifier"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&germplasm;epgrisTerm">EXPERIMENT_NUMBER</skos:altLabel>
    <skos:altLabel xml:lang="en">Experiment number</skos:altLabel>
    <dcterms:source rdf:resource="http://www.epgris3.eu/docs/activities/2-05/Inclusion%20of%20C&amp;E%20data.pdf"/>
    <rdfs:range rdf:resource="dcterms:identifier"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;traitExperimentRemarks">
    <skos:prefLabel xml:lang="en">trait experiment remarks</skos:prefLabel>
    <skos:definition xml:lang="en">Information relevant for the interpretation of the scores in the experiment.</skos:definition>
    <skos:example xml:lang="en">Experimental design, location, experimentor, weather, etc.</skos:example>
    <skos:note xml:lang="en">General remarks related to the experimental conditions or experiment design.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/traitExperimentRemarks"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;MeasurementExperiment"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;GermplasmExperimentRemarks"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&germplasm;epgrisTerm">EXPERIMENT_DESCRIPTION</skos:altLabel>
    <skos:altLabel xml:lang="en">Experiment description</skos:altLabel>
    <dcterms:source rdf:resource="http://www.epgris3.eu/docs/activities/2-05/Inclusion%20of%20C&amp;E%20data.pdf"/>
    <skos:relatedMatch rdf:resource="dwc:measurementRemarks"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;traitExperimentYear">
    <skos:prefLabel xml:lang="en">trait experiment year</skos:prefLabel>
    <skos:definition xml:lang="en">The year when the trait experiment was conducted.</skos:definition>
    <skos:example xml:lang="en">2010</skos:example>
    <skos:note xml:lang="en">For the more detailed experiment date, best practice is to use the dwc:measurementDeterminedDate term.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/traitExperimentYear"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;MeasurementExperiment"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;GermplasmExperimentYear"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&germplasm;epgrisTerm">EXPERIMENT_YEAR</skos:altLabel>
    <skos:altLabel xml:lang="en">Experiment year</skos:altLabel>
    <dcterms:source rdf:resource="http://www.epgris3.eu/docs/activities/2-05/Inclusion%20of%20C&amp;E%20data.pdf"/>
    <skos:relatedMatch rdf:resource="dwc:measurementDeterminedDate"/>
    <skos:relatedMatch rdf:resource="dwc:year"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;traitExperimentReport">
    <skos:prefLabel xml:lang="en">trait experiment report</skos:prefLabel>
    <skos:definition xml:lang="en">A reference to the report of the experiment, either supplied as the file name or the URL of the report.</skos:definition>
    <skos:example xml:lang="en">URL or reference citation</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to provide the URL to the report.</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/traitExperimentReport"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;MeasurementExperiment"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;GermplasmExperimentReport"/>
    <skos:altLabel xml:lang="en" rdf:datatype="&germplasm;epgrisTerm">EXPERIMENT_REPORT</skos:altLabel>
    <skos:altLabel xml:lang="en">Experiment report</skos:altLabel>
    <dcterms:source rdf:resource="http://www.epgris3.eu/docs/activities/2-05/Inclusion%20of%20C&amp;E%20data.pdf"/>
  </skos:Concept>


  <!-- Measurement score -->

  <skos:Concept rdf:about="&germplasm;traitMeasurementByInstituteID">
    <skos:prefLabel xml:lang="en">Trait measurement by institute</skos:prefLabel>
    <skos:definition xml:lang="en">Trait observation determined by institute code, INSTCODE. FAO WIEWS institute code for the institute performing the observation/measurement.</skos:definition>
    <skos:example xml:lang="en">DEU146; NLD037; SWE054; urn:lsid:biocol.org:col:35293</skos:example>
    <skos:note xml:lang="en">Recommended best practice is to use institute codes from the FAO WIEWS information system, "http://apps3.fao.org/wiews/institute_query.htm?i_l=EN" or the Biodiversity Collections Index (BCI), "http://www.biodiversitycollectionsindex.org/".</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/traitMeasurementByInstituteID"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="dwc:MeasurementOrFact"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;MeasurementByInstituteCode"/>
    <rdfs:range rdf:resource="dcterms:identifier"/>
    <skos:relatedMatch rdf:resource="dwc:measurementDeterminedBy"/>
  </skos:Concept>

  <skos:Concept rdf:about="&germplasm;traitMeasurementGrowthStage">
    <skos:prefLabel xml:lang="en">trait measurement at growth stage</skos:prefLabel>
    <skos:definition xml:lang="en">The growth stage of the plant when the trait observation or measurement was made.</skos:definition>
    <skos:example xml:lang="en">flowering; heading; ripening; etc.</skos:example>
    <skos:note xml:lang="en">A more general modifier of the TDWG SDD style could be applied here and include modifiers such as part of the plant where the observation was made (eg. leaf, stem, seeds).</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/traitMeasurementGrowthStage"/>
    <dcterms:created>2009-08-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="dwc:MeasurementOrFact"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:replaces rdf:resource="&germplasm01;MeasurementGrowthStage"/>
  </skos:Concept>






<!-- ### Datatype declaration, Internal annotation properties ###
 Some internal annotation properties to provide the type of identifiers and alternative labels (skos:altLabel)
 There are different types of identifiers and declaring a datatype to keep them appart might perhaps be useful? 
 Examples how to declare datatypes for WIEWS and BCI identifiers:
  <dcterms:identifier rdf:datatype="&germplasm;bioColID">http://biocol.org/urn:lsid:biocol.org:col:35293</dcterms:identifier>
  <dcterms:identifier rdf:datatype="&germplasm;wiewsInstituteID">SWE054</dcterms:identifier>
  <skos:altLabel xml:lang="en" rdf:datatype="&germplasm;mcpdTerm">DONORNUMB</skos:altLabel>
-->

  <rdfs:Datatype rdf:about="&germplasm;wiewsInstituteID">
    <skos:prefLabel xml:lang="en">FAO WIEWS Institute Code</skos:prefLabel>
    <skos:definition xml:lang="en">A three letter country ISO code and a three (old) or four digit number.</skos:definition>
    <skos:example xml:lang="en">ITA247; ITA406; NOR051; SWE002; SWE054</skos:example>
    <skos:note xml:lang="en">The FAO WIEWS Institute Codes are available at http://apps3.fao.org/wiews/institute_query.htm</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/wiewsInstituteID"/>
    <dcterms:created>2012-05-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;DatatypeProperty"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:isReplacedBy rdf:resource="&germplasm;wiewsInstituteID"/>
    <skos:closeMatch rdf:resource="dcterms:identifier"/>
    <skos:relatedMatch rdf:resource="http://apps3.fao.org/wiews/wiews.jsp"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Datatype"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdfs:Datatype>

  <rdfs:Datatype rdf:about="&germplasm;bioColID">
    <skos:prefLabel xml:lang="en">Biodiversity Collections Index ID</skos:prefLabel>
    <skos:definition xml:lang="en">An index of biodiversity collections, providing persistent identifiers based on LSID URIs.</skos:definition>
    <skos:example xml:lang="en">http://biocol.org/urn:lsid:biocol.org:col:35293</skos:example>
    <skos:note xml:lang="en">The Biodiversity Collections Index is available at http://biocol.org/</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/bioColID"/>
    <dcterms:created>2012-05-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;DatatypeProperty"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
    <dcterms:isReplacedBy rdf:resource="&germplasm;bioCollID"/>
    <skos:closeMatch rdf:resource="dcterms:identifier"/>
    <skos:relatedMatch rdf:resource="http://biocol.org/"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Datatype"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdfs:Datatype>

  <rdfs:Datatype rdf:about="&germplasm;mcpdTerm">
    <skos:prefLabel xml:lang="en">Equivalent MCPD term</skos:prefLabel>
    <skos:definition xml:lang="en">Equivalent term from the MultiCrop Passport Descriptor list.</skos:definition>
    <skos:example xml:lang="en">INSTCODE; ACCENUMB; COLLNUMB; etc.</skos:example>
    <skos:note xml:lang="en">The MCPD terms are available at "http://apps3.fao.org/wiews/mcpd/MCPD_Dec2001_EN.pdf"</skos:note>
    <skos:scopeNote rdf:resource="http://code.google.com/p/darwincore-germplasm/wiki/mcpdTerm"/>
    <dcterms:created>2012-05-24</dcterms:created>
    <dcterms:modified>2012-05-24</dcterms:modified>
    <vann:termGroup rdf:resource="&germplasm;DatatypeProperty"/>
    <rdfs:isDefinedBy rdf:resource="&germplasm;"/>
    <skos:inScheme rdf:resource="&germplasm;"/>
    <vs:term_status>unstable</vs:term_status>
