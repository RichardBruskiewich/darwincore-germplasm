  <skos:Collection rdf:about="&germplasm;BreedingEvent">
    <skos:prefLabel xml:lang="en">Breeding event</skos:prefLabel>
    <rdfs:comment xml:lang="en">Breeding event for domesticated and cultivated germplasm material.</rdfs:comment>
    <skos:narrowMatch rdf:resource="dwc:Event"/>
    <skos:narrowMatch rdf:resource="dcmitype:Event"/>
    <skos:relatedMatch rdf:resource="dwc:Occurrence"/>
    <skos:member rdf:resource="&germplasm;BreedingEventID"/>
    <skos:member rdf:resource="&germplasm;BreedersIdentifier"/>
    <skos:member rdf:resource="&germplasm;BreederInstitute"/>
    <skos:member rdf:resource="&germplasm;BreederInstituteCode"/>
    <skos:member rdf:resource="&germplasm;BreederPerson"/>
    <skos:member rdf:resource="&germplasm;BreedingCountry"/>
    <skos:member rdf:resource="&germplasm;BreedingCountryCode"/>
    <skos:member rdf:resource="&germplasm;BreedingYear"/>
