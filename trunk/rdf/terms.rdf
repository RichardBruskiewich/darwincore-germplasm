    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </skos:Concept>



  <!-- Collecting event -->

  <skos:Concept rdf:about="&germplasm;collectingInstituteID">
    <skos:prefLabel xml:lang="en">collecting institute ID</skos:prefLabel>
    <skos:definition xml:lang="en">Collecting institute ID.</skos:definition>
