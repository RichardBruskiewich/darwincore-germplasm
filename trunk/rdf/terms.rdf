    <skos:member rdf:resource="&germplasm;DonorInstitute"/>
    <skos:member rdf:resource="&germplasm;DonorInstituteCode"/>
    <skos:member rdf:resource="&germplasm;DonorsSampleIdentifier"/>
    <skos:member rdf:resource="&germplasm;SampleAcquisitionDate"/>
