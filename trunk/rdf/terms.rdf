<!-- ### Collection of Terms ###
 Labelled collections can be used to group the terms under this sub-heading.

 Each group of terms (skos:Collection) uses the following properties:
  @rdf:about 			(provides the identifier for the group of terms)
  rdfs:label 			(human readable label to be used for the group of terms)
  rdfs:comment			(description of the group of terms)
  skos:relatedMatch		(when the grouping concept has a related match to another concept [in another conceptScheme])
  skos:exactMatch		(when the grouping concept has an exact match to another concept [in another conceptScheme])
  skos:closeMatch		(when the grouping concept has a close match to another concept [in another conceptScheme])
  skos:narrowMatch		(when the grouping concept has a narower hierarchal match to another concept [in another conceptScheme])
  skos:related			(when the grouping concept has a related match to another concept)
  skos:narrower			(when the grouping concept has a narrower hierarchal match to another concept)
  skos:member			(list terms to be organized in each group) [on term, here using vann:termGroup]
  rdf:type				(these entities can be used as Class resources)
 -->

  <skos:Collection rdf:about="&germplasm;Dataset">
    <skos:prefLabel xml:lang="en">Dataset</skos:prefLabel>
    <rdfs:comment xml:lang="en">Dataset with specimens or accessions from genebank collections.</rdfs:comment>
    <skos:closeMatch rdf:resource="dcmitype:Dataset"/>
    <skos:relatedMatch rdf:resource="dcmitype:Collection"/>
    <skos:member rdf:resource="dwc:datasetID"/>
    <skos:member rdf:resource="dwc:datasetName"/>
    <skos:member rdf:resource="dwc:collectionID"/>
    <skos:member rdf:resource="dwc:collectionCode"/>
    <skos:member rdf:resource="dwc:institutionID"/>
    <skos:member rdf:resource="dwc:institutionCode"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </skos:Collection>

  <skos:Collection rdf:about="&germplasm;Taxon">
    <skos:prefLabel xml:lang="en">Taxon</skos:prefLabel>
    <rdfs:comment xml:lang="en">Taxon names for specimens or accessions in genebank collections.</rdfs:comment>
    <skos:exactMatch rdf:resource="dwc:Taxon"/>
    <skos:member rdf:resource="dwc:TaxonID"/>
    <skos:member rdf:resource="dwc:scientificNameID"/>
    <skos:member rdf:resource="dwc:scientificName"/>
    <skos:member rdf:resource="dwc:genus"/>
    <skos:member rdf:resource="dwc:scientificNameAuthorship"/>
    <skos:member rdf:resource="dwc:vernacularName"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </skos:Collection>

  <skos:Collection rdf:about="&germplasm;GermplasmAccession">
    <skos:prefLabel xml:lang="en">Germplasm accession</skos:prefLabel>
    <rdfs:comment xml:lang="en">Genebank specimen or accession is the unit for storing germplasm material in an ex situ genebank collection.</rdfs:comment>
    <skos:closeMatch rdf:resource="dsw:Specimen"/>
    <skos:narrowMatch rdf:resource="dwc:Occurrence"/>
    <skos:relatedMatch rdf:resource="dcmitype:PhysicalObject"/>
    <skos:member rdf:resource="dwc:occurrenceID"/>
    <skos:member rdf:resource="dwc:catalogNumber"/>
    <skos:member rdf:resource="&germplasm;germplasmID"/>
    <skos:member rdf:resource="&germplasm;germplasmIdentifier"/>
    <skos:member rdf:resource="&germplasm;biologicalStatusOfSample"/>
    <skos:member rdf:resource="&germplasm;biologicalStatusOfSampleCode"/>
    <skos:member rdf:resource="&germplasm;typeOfStorage"/>
    <skos:member rdf:resource="dwc:otherCatalogNumbers"/>
    <skos:member rdf:resource="dwc:occurrenceDetails"/>
    <skos:member rdf:resource="dwc:occurrenceRemarks"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </skos:Collection>

  <skos:Collection rdf:about="&germplasm;CollectingEvent">
    <skos:prefLabel xml:lang="en">Collecting event</skos:prefLabel>
    <rdfs:comment xml:lang="en">Collecting event when the germplasm specimen was collected from a natural habitat (in situ) including the farmers field for cultivated germplasm material.</rdfs:comment>
    <skos:narrowMatch rdf:resource="dwc:Event"/>
    <skos:narrowMatch rdf:resource="dcmitype:Event"/>
    <skos:relatedMatch rdf:resource="dwc:Occurrence"/>
    <skos:relatedMatch rdf:resource="dcterms:Location"/>
    <skos:member rdf:resource="dwc:eventID"/>
    <skos:member rdf:resource="dwc:recordNumber"/>
    <skos:member rdf:resource="geo:lat"/>
    <skos:member rdf:resource="geo:long"/>
    <skos:member rdf:resource="geo:alt"/>
    <skos:member rdf:resource="geo:location"/>
    <skos:member rdf:resource="dwc:decimalLatitude"/>
    <skos:member rdf:resource="dwc:decimalLongitude"/>
    <skos:member rdf:resource="dwc:geodeticDatum"/>
    <skos:member rdf:resource="dwc:minimumElevationInMeters"/>
    <skos:member rdf:resource="dwc:eventDate"/>
    <skos:member rdf:resource="dwc:locality"/>
    <skos:member rdf:resource="dwc:countryCode"/>
    <skos:member rdf:resource="dwc:verbatimLatitude"/>
    <skos:member rdf:resource="dwc:verbatimLongitude"/>
    <skos:member rdf:resource="dwc:verbatimCoordinateSystem"/>
    <skos:member rdf:resource="&germplasm;collectingInstituteID"/>
    <skos:member rdf:resource="dwc:recordedBy"/>
    <skos:member rdf:resource="dwc:eventRemarks"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
